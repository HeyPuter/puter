{
    init: [
        {
            pwd: '.',
            command: 'npx webpack'
        },
        {
            pwd: '../phoenix',
            command: 'npx webpack'
        },
    ],
    services: [
        {
            name: 'term.webpack',
            pwd: '.',
            command: 'npx webpack --watch',
        },
        {
            name: 'shell.webpack',
            command: 'npx webpack --watch',
            pwd: '../phoenix'
        },
        {
            name: 'term.http',
            pwd: './dist',
            command: 'npx http-server -p 8082 -c-1',
        },
        {
            name: 'shell.http',
            pwd: '../phoenix/dist',
            // command: 'npx http-server -p 8080 -S -C "{cert}" -K "{key}"',
            command: 'npx http-server -p 8080 -c-1',
        },
    ],
}
