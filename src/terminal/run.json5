{
    services: [
        {
            name: 'term.http',
            pwd: './dist',
            command: 'npx http-server -p 8082 -S -C "{cert}" -K "{key}"',
        },
        {
            name: 'ansi.http',
            pwd: '../phoenix/dist',
            command: 'npx http-server -p 8080 -S -C "{cert}" -K "{key}"',
        },
        {
            name: 'term.webpack',
            command: 'npx webpack --watch',
            pwd: '.'
        },
        {
            name: 'ansi.webpack',
            command: 'npx webpack --watch',
            pwd: '../phoenix'
        },
    ]
}